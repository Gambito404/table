{
    "name": "gambito404/livewire-tables",
    "description": "Headless Livewire table generator compatible with Livewire 3 and 4",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "livewire",
        "tables",
        "datatable",
        "generator"
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Gambito404\\LivewireTables\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gambito404\\LivewireTables\\LivewireTableServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
